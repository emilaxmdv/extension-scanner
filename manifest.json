{
  "manifest_version": 3,
  "name": "Extension Scanner",
  "description": "Scan extensions for risk score.",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "management"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
  "default_popup": "popup.html",
  "default_icon": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"

  },
  "default_title": "Extension Scanner"
},
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"

  }
}
